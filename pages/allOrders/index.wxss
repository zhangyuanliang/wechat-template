.order-wrap {
  min-height: 100vh;
  background: #f5f5f5;
}

.date-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 32rpx 0;
  border-bottom: 1px solid #eee;
}

.date-info {
  width: 100%;
  padding-left: 28rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #202020;
  display: flex;
  align-items: center;
}
